class ForeignSubs extends Subs {
  constructor() {}

  method int monthlyCost()
    return this.channels * 2

  method ForeignSubs foreigners() {
    ForeignSubs out := new ForeignSubs();
    Subs temp := this.next;
    if (temp != nil) then out.next := temp.foreigners();
    out.channels := this.channels;
    return out
  }
}
