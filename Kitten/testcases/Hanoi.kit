class Hanoi {
  constructor() {}

  method void solve(int h, int from, int to, int support)
    if (h < 1) then return
    else if (h = 1) then
      "from ".concat(from).concat(" to ").concat(to).concat("\n").output()
    else {
      this.solve(h - 1, from, support, to);
      "from ".concat(from).concat(" to ").concat(to).concat("\n").output();
      this.solve(h - 1, support, to, from)
    }

  method void main()
    new Hanoi().solve(5,1,2,3)
}
